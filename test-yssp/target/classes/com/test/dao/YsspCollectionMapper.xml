    <select id="queryPersonalCollection" resultMap="theYsspCollectionBelongs" >
		select c.*,i.*,u.*,f.*,n.* from yssp_collection c INNER JOIN yssp_interactive i 
		on c.collection_user_id = #{userId} 
		and c.collection_status = 1 AND c.collection_works_id = i.Interactive_id 
		INNER JOIN qy_user u ON u.ID = i.Interactive_memberId
		LEFT JOIN yssp_follow f ON c.collection_user_id = f.follow_to_people 
		AND i.Interactive_memberId = f.follow_concerned_people 
		LEFT JOIN yssp_user_real_name n ON u.ID = n.real_name_userId 
		ORDER BY c.collection_created_time DESC
  	</select>
  	
    <select id="queryPersonalWorks" resultMap="theYsspCollectionWorksBelongs" >
		select c.*,a.*,n.* from yssp_collection c INNER JOIN yssp_artwork a 
		ON c.collection_works_id = a.artwork_id 
		and c.collection_user_id=#{userId}
		AND c.collection_status = 1 
		INNER JOIN yssp_user_real_name n 
		ON a.artwork_authorId = n.real_name_userId
  	</select>
  	
