  <select id="selectAccreditByConcerned" resultMap="ResultMapWithBLOBs">
  	SELECT 
		ya.*
	FROM
		yssp_accredit ya
	WHERE
		ya.accredit_status =1
	AND
		ya.accredit_concerned_people =#{concernedId,jdbcType=BIGINT}
	ORDER BY
		ya.accredit_created_time DESC
  </select>
  <select id="selectGrantedList" resultMap="ResultMapWithBLOBs">
  	SELECT 
		ya.*,
		qu.*
	FROM
		yssp_accredit ya
	LEFT JOIN
		qy_user qu ON ya.accredit_to_people = qu.ID
	WHERE
		ya.accredit_status =1
	AND
		ya.accredit_concerned_people =#{concernedId,jdbcType=BIGINT}
	ORDER BY
		ya.accredit_created_time 
